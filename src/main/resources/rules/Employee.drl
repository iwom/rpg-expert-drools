package rules;
dialect  "mvel"
import sample.Department;
import sample.Employee;

rule "Print cross product"
    when
        dept: Department()
        emp : Employee(department == dept, manager == true)
    then
        emp.setMessage("I have a laptop!");
        System.out.println(dept.getName() + "::" + emp.getName() + ": -> " + emp.getMessage());
end